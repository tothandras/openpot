{
  "name": "openpot",
  "version": "0.1.0",
  "license": "MIT",
  "dependencies": {
    "angular": "latest",
    "angular-ui-router": "^0.2.13",
    "components-font-awesome": "^4.3.0",
    "angular-mocks": "^1.3.15",
    "normalize.css": "^3.0.3",
    "angular-local-storage": "^0.1.5",
    "angular-material": "^0.8.3",
    "angular-messages": "^1.3.15",
    "angular-notification": "^1.0.1",
    "angular-google-maps": "^2.0.19",
    "angular-md5": "~0.1.7"
  },
  "overrides": {
    "angular": {
      "main": {
        "development": "angular.js",
        "production": "angular.min.js"
      }
    },
    "angular-mocks": {
      "main": {
        "development": "angular-mocks.js",
        "production": ""
      }
    },
    "angular-messages": {
      "main": {
        "development": "angular-messages.js",
        "production": "angular-messages.min.js"
      }
    },
    "angular-ui-router": {
      "main": {
        "development": "release/angular-ui-router.js",
        "production": "release/angular-ui-router.min.js"
      }
    },
    "angular-material": {
      "main": {
        "development": [
          "angular-material.js",
          "angular-material.css"
        ],
        "production": [
          "angular-material.min.js",
          "angular-material.min.css"
        ]
      }
    },
    "angular-google-maps": {
      "main": {
        "development": "dist/angular-google-maps.js",
        "production": "dist/angular-google-maps.min.js"
      }
    },
    "angular-notification": {
      "main": {
        "development": "angular-notification.js",
        "production": "angular-notification.min.js"
      }
    },
    "angular-md5": {
      "main": {
        "development": "angular-md5.js",
        "production": "angular-md5.min.js"
      }
    },
    "components-font-awesome": {
      "main": [
        "fonts/fontawesome-webfont.eot",
        "fonts/fontawesome-webfont.woff",
        "fonts/fontawesome-webfont.woff2",
        "fonts/fontawesome-webfont.ttf",
        "fonts/fontawesome-webfont.svg"
      ]
    },
    "lodash": {
      "main": {
        "development": "dist/lodash.js",
        "production": "dist/lodash.min.js"
      }
    }
  }
}
