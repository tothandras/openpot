{
  "name": "openpot",
  "version": "0.1.0",
  "license": "MIT",
  "dependencies": {
    "angular": "latest",
    "angular-ui-router": "~0.2.13",
    "components-font-awesome": "~4.3.0",
    "angular-mocks": "~1.3.13",
    "normalize.css": "~3.0.2",
    "angular-socket-io": "~0.7.0",
    "socket.io-client": "~1.3.4",
    "angular-local-storage": "~0.1.5"
  },
  "overrides": {
    "angular": {
      "main": {
        "development": "angular.js",
        "production": "angular.min.js"
      }
    },
    "angular-mocks": {
      "main": {
        "development": "angular-mocks.js",
        "production": ""
      }
    },
    "angular-ui-router": {
      "main": {
        "development": "release/angular-ui-router.js",
        "production": "release/angular-ui-router.min.js"
      }
    },
    "socket.io-client": {
      "main": "socket.io.js"
    },
    "angular-socket-io": {
      "dependencies": {
        "socket.io-client": "*"
      },
      "main": {
        "development": "socket.js",
        "production": "socket.min.js"
      }
    },
    "components-font-awesome": {
      "main": [
        "fonts/fontawesome-webfont.eot",
        "fonts/fontawesome-webfont.woff",
        "fonts/fontawesome-webfont.ttf",
        "fonts/fontawesome-webfont.svg"
      ]
    }
  }
}
